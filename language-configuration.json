{
    "comments": {
      "lineComment": "//",
      "blockComment": [ "//- ", "" ]
    },
    "brackets": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"]
    ],
    "autoClosingPairs": [
      { "open": "{", "close": "}" },
      { "open": "[", "close": "]" },
      { "open": "(", "close": ")" },
      { "open": "\"", "close": "\"", "notIn": ["string"] },
      { "open": "'", "close": "'", "notIn": ["string"] }
    ],
    "surroundingPairs": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"],
      ["\"", "\""],
      ["'", "'"]
    ],
    "indentationRules": {
      "increaseIndentPattern": "^\\s*([\\w\\-]+)?(\\.|\\#[\\w\\-]+)*(\\([^)]*\\))?(\\:[\\w\\-]+)?\\s*$",
      "decreaseIndentPattern": "^\\s*$"
    },
    "folding": {
      "markers": {
        "start": "^\\s*//\\s*#?region\\b",
        "end": "^\\s*//\\s*#?endregion\\b"
      }
    }
  }